<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.icip.framework.common.ApplicationContextUtil" />
	<!-- RESOURCES -->
	<import resource="classpath:app-resources.xml" />
	
	<import resource="classpath:app-dao.xml" />

	<!-- 自定义BEAN -->
	<import resource="classpath:app-beans.xml" />

	<!-- 信息交互 -->
	<import resource="classpath:app-message.xml" />

</beans>